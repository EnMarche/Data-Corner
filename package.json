  "scripts": {
    "start": "vite",
    "start-test": "yarn rollup:dev & yarn rollup:preview",
    "dev": "vite preview",
    "build": "vite build --mode production",
    "test": "TZ=Europe/Paris jest",
    "test-coverage": "jest src --coverage",
    "test:cy": "cypress open",
    "lint": "eslint './src/**/*.js' --ignore-path .gitignore",
    "lint:fix": "yarn run lint --fix",
    "prepare": "husky install",
    "rollup:dev": "yarn vite build --config rollup.config.js --mode test",
    "rollup:preview": "yarn vite preview --config rollup.config.js"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@cypress/vite-dev-server": "2.2.3",
    "@testing-library/react": "12.1.5",
    "@vitejs/plugin-react": "1.3.2",
    "babel-jest": "28.1.0",
    "cypress": "9.7.0",
    "eslint": "8.17.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "13.0.0",
    "prettier": "2.6.2",
    "vite": "2.9.9",
    "vite-plugin-environment": "1.1.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-html-env": "1.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "moduleNameMapper": {
      "^(api)/(.*)$": "<rootDir>/src/$1/$2",
      "^(assets)/(.*)$": "<rootDir>/src/$1/mock",
      "^(components)/(.*)$": "<rootDir>/src/$1/$2",
      "^(domain)/(.*)$": "<rootDir>/src/$1/$2",
      "^(providers)/(.*)$": "<rootDir>/src/$1/$2",
      "^(shared)/(.*)$": "<rootDir>/src/$1/$2",
      "^(services)/(.*)$": "<rootDir>/src/$1/$2",
      "^(style)/(.*)$": "<rootDir>/src/$1/$2",
      "^(ui)/(.*)$": "<rootDir>/src/$1/$2"
    },
    "testEnvironment": "jsdom"
  }
}
